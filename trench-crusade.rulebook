      "Infantry§Heretic Trooper": {
        "keywords": {
          "Faction": [
            "Heretic Legion"
          ],
          "Keywords": [
            "HERETIC"
          ],
          "Type": [
            "Troop"
          ]
        },
        "rules": {
          "Only half of your Heretic Troopers can upgrade.": {
            "evals": [
              {
                "actionable": true,
                "max": [
                  "{roster}",
                  "tally",
                  "Heretic Trooper"
                ],
                "not": false,
                "operator": "AND",
                "paths": [
                  [
                    "{roster}",
                    "tally",
                    "Legionnaire"
                  ]
                ]
              }
            ],
            "evaluate": "AND",
            "failState": "error"
          },
          "Only one type of Legionnaire allowed (Melee OR Ranged)": {
            "evals": [
              {
                "actionable": true,
                "not": false,
                "operator": "AND",
                "paths": [
                  [
                    "{self}",
                    "stats",
                    "Upgrade",
                    "value"
                  ]
                ],
                "value": "-"
              },
              {
                "actionable": false,
                "min": 1,
                "not": true,
                "operator": "AND",
                "paths": [
                  [
                    "{roster}",
                    "tracked",
                    "Legionnaire (Ranged)"
                  ]
                ]
              },
              {
                "actionable": false,
                "not": true,
                "operator": "AND",
                "paths": [
                  [
                    "{roster}",
                    "tracked",
                    "Legionnaire (Ranged)"
                  ]
                ],
                "value": 0
              },
              {
                "actionable": false,
                "min": 1,
                "not": true,
                "operator": "AND",
                "paths": [
                  [
                    "{roster}",
                    "tracked",
                    "Legionnaire (Melee)"
                  ]
                ]
              },
              {
                "actionable": false,
                "not": true,
                "operator": "AND",
                "paths": [
                  [
                    "{roster}",
                    "tracked",
                    "Legionnaire (Melee)"
                  ]
                ],
                "value": 0
              },
              {
                "actionable": true,
                "not": false,
                "operator": "AND",
                "paths": [
                  [
                    "{this}",
                    "evals",
                    "1",
                    "result",
                    "truthy"
                  ],
                  [
                    "{this}",
                    "evals",
                    "4",
                    "result",
                    "truthy"
                  ]
                ],
                "value": true
              },
              {
                "actionable": true,
                "not": false,
                "operator": "AND",
                "paths": [
                  [
                    "{this}",
                    "evals",
                    "2",
                    "result",
                    "truthy"
                  ],
                  [
                    "{this}",
                    "evals",
                    "3",
                    "result",
                    "truthy"
                  ]
                ],
                "value": true
              }
            ],
            "evaluate": "OR",
            "failState": "error"
          },
          "UpdateDesignation": {
            "actions": [
              {
                "actionType": "set",
                "iterations": 1,
                "paths": [
                  [
                    "{self}",
                    "designation"
                  ]
                ],
                "prepend": " ",
                "value": "Legionnaire"
              }
            ],
            "evals": [
              {
                "actionable": true,
                "contains": true,
                "not": false,
                "operator": "AND",
                "paths": [
                  [
                    "{self}",
                    "stats",
                    "Upgrade",
                    "value"
                  ]
                ],
                "value": "Legionnaire"
              }
            ],
            "evaluate": "AND",
            "failState": "pass",
            "order": 999
          }
        },
