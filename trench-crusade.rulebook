                "value": "-"
              },
              {
                "actionable": false,
                "min": 1,
                "not": true,
                "operator": "AND",
                "paths": [
                  [
                    "{roster}",
                    "tracked",
                    "Legionnaire (Ranged)"
                  ]
                ]
              },
              {
                "actionable": false,
                "not": true,
                "operator": "AND",
                "paths": [
                  [
                    "{roster}",
                    "tracked",
                    "Legionnaire (Ranged)"
                  ]
                ],
                "value": 0
              },
              {
                "actionable": false,
                "min": 1,
                "not": true,
                "operator": "AND",
                "paths": [
                  [
                    "{roster}",
                    "tracked",
                    "Legionnaire (Melee)"
                  ]
                ]
              },
              {
                "actionable": false,
                "not": true,
                "operator": "AND",
                "paths": [
                  [
                    "{roster}",
                    "tracked",
                    "Legionnaire (Melee)"
                  ]
